{
  "name": "@koakh/nestjs-package-jwt-authentication-graphql",
  "version": "1.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "start:dev": "tsc-watch -p tsconfig.build.json",
    "start:dev-and-app": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "version:patch": "npm run build && npm version patch",
    "version:publish": "npm publish"
  },
  "keywords": [
    "nestjs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.0.0"
  },
  "dependencies": {
    "@nest-middlewares/cookie-parser": "^6.0.0",
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/graphql": "^8.0.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.6",
    "bcrypt": "^5.0.1",
    "graphql-subscriptions": "^1.2.1",
    "graphql-type-json": "^0.3.2",
    "graphql": "^15.5.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport": "^0.4.1",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.0.6",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.13",
    "@types/supertest": "^2.0.11",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.3.0",
    "supertest": "^6.1.4",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "tsc-watch": "^4.4.0",
    "tsconfig-paths": "^3.10.1",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
